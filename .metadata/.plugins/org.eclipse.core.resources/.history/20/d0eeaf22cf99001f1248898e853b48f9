#include <stdint.h>

#define ADC_BASE_ADDR        0x40012400UL
#define ADC_CR1_OFFSET       0x08UL
#define ADC_CR1_ADDR 		 (ADC_BASE_ADDR + ADC_CR1_OFFSET)

#define RCC_BASE_ADDR 			0x40021000UL
#define RCC_APB_ENR2_OFFSET		0x40UL;
#define RCC_APB_ENR2_ADDR		(RCC_BASE_ADDR + RCC_APB_ENR2_OFFSET)

#if !defined(__SOFT_FP__) && defined(__ARM_FP)
  #warning "FPU is not initialized, but the project is compiling for an FPU. Please initialize the FPU before use."
#endif

int main(void)
{
	uint32_t *pAdc_cr1_addr = (uint32_t*)ADC_CR1_ADDR;

	uint32_t *prcc_apb_enr2_addr = (uint32_t*)RCC_APB_ENR2_ADDR;

	// 1. Enable the RCC register of corresponding peripheral you want to use
	*prcc_apb_enr2_addr |= (1<<20);

	*pAdc_cr1_addr |= (1<<31);

    /* Loop forever */
	for(;;);
}
